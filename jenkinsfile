pipeline{
    agent any
    tools{
        maven 'Maven'
        jdk 'Java_11'
    }
    stages{
        stage("Git Checkout")
        {
            steps{
                git branch: 'main' , url: 'https://github.com/ManojKRISHNAPPA/test-1.git'
                }
        }
        
        stage("Maven clean")
        {
            steps{
                sh 'mvn clean'
                }
        }
        
        stage("Maven Build")
        {
            steps{
                sh 'mvn clean install'
                }
        }

        }
}
