pipeline{
    agent any

    stages{
        stage("Git Checkout")
        {
            steps{
                git branch: 'test' , url: 'https://github.com/OmkarKuratti/Devops_Practice.git'
                }
        }
     
        
        stage("Shell Script")
        {
            steps{
                sh './test.sh'
                }
        }

        }
}
